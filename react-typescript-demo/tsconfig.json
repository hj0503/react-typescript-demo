{
    "compilerOptions": {
      "target": "es6",
      "module": "es6",
      "moduleResolution": "node",
      "jsx": "react",
      "baseUrl": ".",
      "allowSyntheticDefaultImports": true,
      "paths": {
        "components/*": [
          "src/components/*"
        ],
        "data/*": [
          "src/data/*"
        ],
        "layouts/*": [
          "src/layouts/*"
        ],
        "models/*": [
          "src/models/*"
        ],
        "selector/*": [
          "src/selector/*"
        ],
        "services/*": [
          "src/services/*"
        ],
        "utils/*": [
          "src/utils/*"
        ]
      }
    },
    "include": [
      "./src/**/*"
    ] 
  }
  